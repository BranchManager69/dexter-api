# Required database connection
DATABASE_URL=postgresql://postgres:password@host:6543/postgres?sslmode=require&options=-c%20search_path%3Dpublic
# Optional session pool (used automatically for Prisma migrations when set)
DATABASE_URL_SESSION=postgresql://postgres:password@host:5432/postgres?sslmode=require&options=-c%20search_path%3Dpublic

# Supabase configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=stub
SUPABASE_SERVICE_ROLE_KEY=stub
SUPABASE_JWT_SECRET=stub

# OpenAI configuration
OPENAI_API_KEY=sk-your-key
OPENAI_API_BASE=https://api.openai.com
OPENAI_REALTIME_MODEL=gpt-realtime
TEXT_MODEL=gpt-5-mini

# MCP routing
MCP_URL=https://dexter.cash/mcp
TOKEN_AI_MCP_TOKEN=
MCP_ALLOWED_TOOLS_CHAT=
MCP_ALLOWED_TOOLS_VOICE=

# Connector OAuth (OIDC client settings)
TOKEN_AI_OIDC_CLIENT_ID=
TOKEN_AI_OIDC_CLIENT_ID_CHATGPT=
TOKEN_AI_OIDC_SCOPES=openid profile email wallet.read wallet.trade
TOKEN_AI_OIDC_ISSUER=
TOKEN_AI_OIDC_CHATGPT_HOSTS=dexter.cash,mcp.dexter.cash,branch.bet

# Required for per-user MCP auth. Provide a long random string; the token endpoint signs
# a short-lived Dexter MCP JWT alongside the Supabase tokens.
MCP_JWT_SECRET=
MCP_JWT_TTL_SECONDS=900

# Connector OAuth (optional overrides)
# Required secret used when issuing deterministic authorization codes.
# Generate a long random string in production (e.g. `openssl rand -hex 32`).
CONNECTOR_CODE_SALT=change-me-with-a-random-64-char-string
# Allowlists accept comma-separated values (leave blank to use defaults).
CONNECTOR_ALLOWED_REDIRECTS=
CONNECTOR_ALLOWED_CLIENT_IDS=
# CONNECTOR_MOBILE_REDIRECTS accepts comma-separated templates in the form
# client_id|https://example.com/deeplink?continue={{redirect_url_encoded}}
# Available placeholders: {{code}}, {{code_encoded}}, {{state}}, {{state_encoded}},
# {{redirect_uri}}, {{redirect_uri_encoded}}, {{redirect_url}}, {{redirect_url_encoded}},
# {{platform}}, {{platform_encoded}}
CONNECTOR_MOBILE_REDIRECTS=

# Server settings
PORT=3030
ALLOWED_ORIGINS=*
STREAM_SCENE_PASSWORD=0727
MEMORY_SUMMARIZER_INTERVAL_MS=30000
REALTIME_BROADCAST_TOKEN=

# Solana trading
SOLANA_RPC_ENDPOINT=https://api.mainnet-beta.solana.com
DEXTER_TOKEN_MINT=
WALLET_ENCRYPTION_KEY=
DEXTER_SOLANA_TREASURY=
DEXTER_PLATFORM_FEE_BPS_FREE=200
DEXTER_PLATFORM_FEE_BPS_PRO=0
# Optional: shared demo wallet for guest sessions (public key)
TOKEN_AI_DEFAULT_WALLET_ADDRESS=
# Optional: set when using paid Jupiter plans
JUPITER_API_HOST=https://lite-api.jup.ag
JUPITER_API_KEY=
