# Dexter API env (copy to .env)

# Required: server-side OpenAI API key for creating Realtime ephemeral sessions
OPENAI_API_KEY=

# Optional: override realtime model
OPENAI_REALTIME_MODEL=gpt-realtime

# Text model for Agents (default fast + strong)
TEXT_MODEL=gpt-5-mini

# Hosted MCP server URL (Streamable HTTP). Defaults to production.
MCP_URL=https://dexter.cash/mcp

# API server
PORT=3030
ALLOWED_ORIGINS=*
