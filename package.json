{
  "name": "dexter-api",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "description": "Dexter API (Agents + Realtime + hosted MCP tools)",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "probe:oauth": "node scripts/oauth-probe.mjs",
    "referral:setup": "node --loader ts-node/esm scripts/jupiter-referral/setupReferral.ts",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx || true",
    "x402:test": "node scripts/pay-solana-test.mjs",
    "prisma": "./scripts/prisma.sh",
    "prisma:status": "./scripts/prisma.sh migrate status --schema prisma/schema.prisma",
    "wallets:import": "tsx scripts/wallets/import.ts",
    "wallets:generate": "tsx scripts/wallets/generate.ts"
  },
  "dependencies": {
    "@openai/agents": "^0.1.0",
    "@openai/agents-core": "^0.1.0",
    "@openai/agents-openai": "^0.1.0",
    "@prisma/client": "^6.7.0",
    "@solana/spl-token": "^0.4.3",
    "@solana/web3.js": "^1.95.3",
    "@supabase/supabase-js": "^2.58.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.19.1",
    "tiktoken": "^1.0.22",
    "x402": "^0.6.1",
    "x402-express": "^0.6.1",
    "yargs": "^18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "eslint": "^9.9.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  }
}
